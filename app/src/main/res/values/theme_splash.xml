<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.Splash" parent="Theme.SplashScreen">
        <item name="windowSplashScreenBackground">@color/primary_color</item>
        <item name="windowSplashScreenAnimatedIcon">@drawable/icone_splash_arredondado</item>
        <item
            name="android:windowSplashScreenBrandingImage"
            tools:targetApi="s">@drawable/logo_splash</item>
        <item name="postSplashScreenTheme">@style/Theme.Project1732</item>
    </style>
</resources>


